{{#paper-toolbar}}
  {{#paper-toolbar-tools}}
    <div class='logo'>
      {{#link-to 'index'}}{{#paper-button}}
        {{paper-icon "home"}}
      Christian	Abide{{/paper-button}}
      {{/link-to}}
    </div>
    <div class="bannerImage">&nbsp;</div>
    <div class='paper-right'>
      {{#link-to 'about'}}{{#paper-button}}
        About CA{{/paper-button}}
      {{/link-to}}
      {{#link-to 'poweredBy'}}{{#paper-button}}
        Built by CCS{{/paper-button}}
      {{/link-to}}
      {{#link-to 'contact'}}{{#paper-button}}
        Contact{{/paper-button}}
      {{/link-to}}
      {{#if casession.currentUser}}
        Welcome {{link-to casession.currentUser.displayName 'members.myHome.main'}}!
        {{#paper-button onClick=(action 'logout')}}
          {{paper-icon "person"}}
          Logout{{/paper-button}}
      {{else}}
        {{#link-to 'login'}}{{#paper-button}}
          {{paper-icon "person"}}
          Login/Register{{/paper-button}}
        {{/link-to}}
      {{/if}}
    </div>
  {{/paper-toolbar-tools}}
{{/paper-toolbar}}
<div class="flash-message">
  {{#each flashMessages.queue as |flash|}}
    {{flash-message flash=flash}}
  {{/each}}
</div>
{{#paper-content class='md-padding'}}
    {{outlet}}
{{/paper-content}}
